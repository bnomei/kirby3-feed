{
    "name": "bnomei/kirby3-feed",
    "type": "kirby-plugin",
    "version": "1.1.3",
    "description": "Generate a RSS/JSON-Feed from a Pages-Collection",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "rss-feed",
        "rss",
        "feed",
        "json"
    ],
    "autoload": {
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/composer-installer": "^1.1"
    }
}
